{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "outDir": "dist",
    "baseUrl": "./",
    "strict": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
  },
   "include": [
      "src/**/*",
      "src/types.d.ts",
  ],
  "exclude": [
      "node_modules"
  ],
  "files":["./src/types.d.ts"]
}
